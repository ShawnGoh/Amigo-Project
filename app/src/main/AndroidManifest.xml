<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infosys1d_amigoproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".projectmanagement.CreateNewProject"></activity>
        <activity android:name=".profilemanagement.profileactivity"></activity>
        <activity android:name=".chat.MessageActivity" />
        <activity android:name=".profilesetup.ProfileSetupSkills" />
        <activity android:name=".profilesetup.ProfileSetupProfilePic" />
        <activity android:name=".profilesetup.ProfileSetupLookingFor" />
        <activity android:name=".profilesetup.ProfileSetupAboutMe" />
        <activity android:name=".profilesetup.ProfileSetupBio" />
        <activity android:name=".signinsignup.SignIn" />
        <activity android:name=".signinsignup.SignUp" />
        <activity android:name=".projectmanagement.ExploreProjectListings">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>
        <activity android:name=".projectmanagement.ProjectDetails" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".onboarding.Onboarding" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>